{
  "name": "chambroad-atlas-electron",
  "version": "0.1.0",
  "description": "Chambroad Atlas - Electron desktop browser with AI automation",
  "main": "dist/main/index.js",
  "author": "Chambroad",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder --mac --dir",
    "start": "electron .",
    "package": "vite build && electron-builder --mac"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/mcp": "^0.0.5",
    "@composio/vercel": "^0.2.14",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "ai": "^5.0.76",
    "electron-store": "^9.0.0",
    "i18next": "^25.7.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "appId": "com.chambroad.atlas",
    "productName": "Chambroad Atlas",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.icns"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    }
  }
}
